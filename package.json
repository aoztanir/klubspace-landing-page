{
	"name": "klubspace.com",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:supabase": "cd $npm_config_SUPABASE_PATH && supabase start",
		"types:local": "cd $npm_config_SUPABASE_PATH && supabase gen types --lang=typescript --local --schema public > \".generated-types/supabase.ts\"",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-embed --plugin=prettier-plugin-sql --check .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-embed --plugin=prettier-plugin-sql --write .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test": "pnpm test:unit && pnpm test:integration",
		"test:unit": "vitest",
		"test:integration": "playwright test",
		"build": "vite build",
		"preview": "vite preview",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@internationalized/date": "^3.6.0",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.0",
		"@playwright/test": "^1.28.1",
		"@supabase/ssr": "^0.5.1",
		"@supabase/supabase-js": "^2.45.4",
		"@sveltejs/adapter-node": "^5.2.5",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@tailwindcss/typography": "^0.5.15",
		"@tiptap/core": "^2.9.1",
		"@tiptap/extension-character-count": "^2.9.1",
		"@tiptap/extension-link": "^2.9.1",
		"@tiptap/extension-underline": "^2.9.1",
		"@tiptap/starter-kit": "^2.9.1",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.57",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.18",
		"embla-carousel-svelte": "^8.5.2",
		"formsnap": "1.0.1",
		"lucide-svelte": "^0.453.0",
		"mode-watcher": "^0.5.0",
		"node-html-markdown": "^1.3.0",
		"paneforge": "^0.0.6",
		"phosphor-svelte": "^3.0.1",
		"postcss": "^8.4.49",
		"postcss-import": "^16.1.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.22.1",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"vaul-svelte": "^0.3.2",
		"vite": "^5.0.3",
		"vitest": "^2.0.0",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"svelte-inview": "^4.0.4"
	}
}
